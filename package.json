{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-StringLimit": "cd nova-components/StringLimit && npm run dev",
        "build-StringLimit-prod": "cd nova-components/StringLimit && npm run prod",
        "build-CurrencyField": "cd nova-components/CurrencyField && npm run dev",
        "build-CurrencyField-prod": "cd nova-components/CurrencyField && npm run prod",
        "build-tabs-additional-characteristics": "cd nova-components/TabsAdditionalCharacteristics && npm run dev",
        "build-tabs-additional-characteristics-prod": "cd nova-components/TabsAdditionalCharacteristics && npm run prod",
        "build-question-options": "cd nova-components/QuestionOptions && npm run dev",
        "build-question-options-prod": "cd nova-components/QuestionOptions && npm run prod",
        "build-product-variants": "cd nova-components/ProductVariants && npm run dev",
        "build-product-variants-prod": "cd nova-components/ProductVariants && npm run prod",
        "build-product-brand": "cd nova-components/ProductBrand && npm run dev",
        "build-product-brand-prod": "cd nova-components/ProductBrand && npm run prod",
        "build-common-questions": "cd nova-components/CommonQuestions && npm run dev",
        "build-common-questions-prod": "cd nova-components/CommonQuestions && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.26.1",
        "bootstrap": "^4.1.0",
        "cross-env": "^5.1",
        "jquery": "^3.5.1",
        "laravel-mix": "^6.0.43",
        "laravel-mix-purgecss": "^6.0.0",
        "lodash": "^4.17.21",
        "popper.js": "^1.12",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "tailwindcss-plugins": "^0.3.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "lottie-web": "^5.7.3",
        "mix-tailwindcss": "^1.2.0",
        "owl.carousel": "^2.3.4",
        "tailwindcss": "^3.0.23",
        "vue-toasted": "^1.1.28",
        "vue-waypoint": "^3.5.0"
    }
}
